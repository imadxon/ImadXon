{
  "name": "WhatsApp Bot IMAD XON",
  "description": "I am IMAD XON WhatsApp MultiDevice Bot built in NodeJs to enhance the experience.",
  "logo": "https://files.catbox.moe/i9qv6b.jpg",
  "keywords": ["IMAD XON"],
  "repository": "https://github.com/JawadTechX/KHAN-MD",
  "stack": "container",
  "env": { 
      "SESSION_ID": {
      "description": "your session id",
      "required": true,
      "value": ""
    },
    "MODE": {
      "description": "mode public or self",
      "required": true,
      "value": "public"
    },
    "AUTO_STATUS_SEEN": {
      "description": "make it true if you want bot to view status",
      "required": false,
      "value": "true"
    },
    "AUTO_STATUS_REPLY": {
      "description": "make it true if you want to send message after seen status",
      "required": false,
      "value": "false"
    },
    "STATUS_READ_MSG": {
      "description": "set your status reply message",
      "required": false,
      "value": "*Status Seen By IMAD XON üëªü§ç*"
    },
    "OWNER_NAME": {
      "description": "set your status reply message",
      "required": false,
      "value": "IMAD XON"
    },
    "OWNER_NUMBER": {
      "description": "set your status reply message",
      "required": false,
      "value": "92310344XXXX"
    },
    "AUTO_READ": {
      "description": "make it true if you want bot to read message",
      "required": false,
      "value": "false"
    },
    "AUTO_TYPING": {
      "description": "make it true if you want bot to show typing",
      "required": false,
      "value": "false"
    },
    "AUTO_RECORDING": {
      "description": "make it true if you want bot to show recording audio",
      "required": false,
      "value": "false"
    },
    "AUTO_REACT": {
      "description": "make it true if want auto react",
      "required": false,
      "value": "false"
    },
    "ALWAYS_ONLINE": {
      "description": "make it true if you want bot to show always online",
      "required": false,
      "value": "false"
    },
    "AUTO_BLOCK": {
      "description": "make it true if you want bot to auto block only 212 numbers",
      "required": false,
      "value": "false"
    },
    "REJECT_CALL": {
      "description": "make it true if you want to reject call",
      "required": false,
      "value": "false"
    },
    "PREFIX": {
      "description": "Your Bot Prefix default (.)",
      "required": false,
      "value": "."
    }
    },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}